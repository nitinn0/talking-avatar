{
  "name": "digital-human",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "apps/*"
  ],
  "scripts": {
    "client": "yarn --cwd apps/frontend dev",
    "server": "yarn --cwd apps/backend dev",
    "dev": "npm-run-all --parallel client server",
    "build:client": "yarn --cwd apps/frontend build",
    "build:server": "yarn --cwd apps/backend build",
    "build": "npm-run-all --parallel build:client build:server"
  },
  "devDependencies": {
    "nodemon": "^3.0.3",
    "npm-run-all": "^4.1.5"
  },
  "dependencies": {
    "yarn": "^1.22.22"
  }
}
